      "regex": "siteName = @\"*.+\"",
      "mapping": "adobe",
      "replace": "NSString *siteName = @\"*.+\";"
    },
    {
      "name": "../ios/Sirius.xcodeproj/project.pbxproj",
      "regex": "DEVELOPMENT_TEAM = *.+;",
